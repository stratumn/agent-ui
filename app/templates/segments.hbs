{{#paper-content}}
  {{segments-filter
    class="filter"
  	mapId=model.mapId
  	prevLinkHash=model.prevLinkHash
  	tags=model.tags
  	onSubmit=(action "userDidUpdateFilter")
  }}
  {{segments-list
  	segments=model.segments
  	hasNoMore=hasNoMore
  	onLoadMore=(action "userDidLoadMore")
  }}
{{/paper-content}}
