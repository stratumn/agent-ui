{{#if showCreateMap}}
  {{maps-create
  	action=model.agent.actions.[0]
  	error=error
  	onClose=(action "userDidToggleShowCreateMap")
  	onSubmit=(action "userDidCreateMap")
  }}
{{/if}}

{{#paper-content}}
  {{maps-list maps=model.maps hasNoMore=hasNoMore onLoadMore=(action "userDidLoadMore")}}
{{/paper-content}}
